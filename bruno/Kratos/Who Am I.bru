meta {
  name: Who Am I
  type: http
  seq: 1
}

get {
  url: {{kratos_public}}/sessions/whoami
  body: none
  auth: inherit
}

headers {
  Accept: application/json
}

script:post-response {
  if (res.getStatus() === 200) {
    const response = res.getBody();
    const id = response?.identity?.id;
    
    if (id) {
      bru.setEnvVar("identity_id", id);
    }
  }
}
