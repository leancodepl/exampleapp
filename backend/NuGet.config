<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear/>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="feedz-leancode-public" value="https://f.feedz.io/leancode/public/nuget/index.json" />
  </packageSources>
  <packageSourceMapping>
